text = "What an incredible aquarium! Such a shame all of these fish and equipment to go to waste...must have cost a fortune!"
metadata/_custom_type_script = "uid://cyus3nuisb47q"

[sub_resource type="Resource" id="Resource_f82ng"]
script = ExtResource("3_hvmjx")
line_type = 1
text = "Oh, do you know much about aquariums?"
metadata/_custom_type_script = "uid://cyus3nuisb47q"

[sub_resource type="Resource" id="Resource_gbark"]
script = ExtResource("3_hvmjx")
line_type = 0
text = "Huh? Aquariums... No, I have no interest in any of that sort of business."
metadata/_custom_type_script = "uid://cyus3nuisb47q"

[sub_resource type="Resource" id="Resource_qrsvt"]
script = ExtResource("3_hvmjx")
line_type = 0
text = "Sorry who are you? Why are you asking me all these probing questions?"
metadata/_custom_type_script = "uid://cyus3nuisb47q"

[sub_resource type="Resource" id="Resource_4ykt3"]
