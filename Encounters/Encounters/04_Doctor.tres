[sub_resource type="Resource" id="Resource_370qv"]
script = ExtResource("4_gis7i")
speaker_sprite = ExtResource("5_khqt7")
line_type = 0
text = "How dreadful about this aquarium....absolutely dreadful....I wonder what will happen to all the fish? Would be a shame for them to all go to waste..."
metadata/_custom_type_script = "uid://cyus3nuisb47q"

[sub_resource type="Resource" id="Resource_6fq06"]
script = ExtResource("4_gis7i")
line_type = 1
text = "Did you know the victim?"
metadata/_custom_type_script = "uid://cyus3nuisb47q"

[sub_resource type="Resource" id="Resource_hjhqv"]
script = ExtResource("4_gis7i")
line_type = 0
text = "Hmmm, yes, I did, 'The great Philanthropist, Phill Anthropist! I'm sorry but are you serious? What an awful name...should be asking him about what happened to his parents."
metadata/_custom_type_script = "uid://cyus3nuisb47q"

[sub_resource type="Resource" id="Resource_dt2c1"]
script = ExtResource("6_w7fsa")
