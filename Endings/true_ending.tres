[sub_resource type="Resource" id="Resource_72ic1"]
script = ExtResource("1_72ic1")
speaker_sprite = ExtResource("3_lunxg")
line_type = 1
text = "... and that's the story of how I caught the killer."
update_background = ExtResource("4_rsbj1")
